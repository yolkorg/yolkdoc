{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "linden",
    "name": "yolkbot",
    "colors": {
        "primary": "#009bdc",
        "light": "#009bdc",
        "dark": "#009bdc"
    },
    "favicon": "/img/icon.png",
    "fonts": {
        "family": "Nunito"
    },
    "appearance": {
        "default": "light"
    },
    "navigation": {
