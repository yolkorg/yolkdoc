                {
                    "anchor": "github repo",
                    "href": "https://github.com/yolkorg/yolkbot",
                    "icon": "github"
                },
                {
                    "anchor": "discord guild",
                    "href": "https://discord.gg/gYugVUCaMr",
                    "icon": "discord"
                }
            ]
        }
